STORE .T. TO WCARGANDO
DO WHILE WCARGANDO
   @ WLIN02+0,WCOL02+0 CLEAR to WLIN02+4,WCOL02+55
   @ WLIN02+0,WCOL02+0       to WLIN02+4,WCOL02+55 DOUBLE
   @ WLIN02+0,WCOL02+5 SAY " UBICACION ADMINISTRATIVA "
   @ WLIN02+1,WCOL02+1  SAY "GRUPO       :"
   @ WLIN02+2,WCOL02+1  SAY "SUBGRUPO    :"
   @ WLIN02+3,WCOL02+1  SAY "RENGLON     :"
   STORE "INGRESE EL CODIGO SOLICITADO ( *ELIMINA*=ELIMINAR CODIGO, <ESC>=SALIR)" TO MES
   DO MENSAJE WITH MES
   @ WLIN02+1,WCOL02+15  GET WADMGRU
   READ
   IF WADMGRU=SPACE(3) .OR. WADMGRU="000" .OR.READKEY()=12.OR.READKEY()=268
      EXIT
   ENDIF
   STORE WADMGRU+"000"+"000" TO WCLAVE
   SELECT 2
   FIND &WCLAVE
   IF EOF()
      DO FILLOC
      APPEND BLANK
      UNLOCK
      DO RECLOC
      REPLACE GRUPO     WITH WADMGRU
      REPLACE SUBGRUPO  WITH "000"
      REPLACE RENGLON   WITH "000"
      FLUSH
      @ WLIN02+1,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
   ELSE
      do recloc
      @ WLIN02+1,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
      IF DESCRI = "*ELIMINA*"
         STORE "ESTA SEGURO QUE DESEA ELIMINAR ? (S/N)" TO TEX
         STORE "NS" TO WCH
         DO PREGUNTA
         IF WCH = "N"
            LOOP
         ELSE
            DO FILLOC
            DELETE
            UNLOCK
            LOOP
         ENDIF
      ENDIF
      FLUSH
   ENDIF
   @ WLIN02+2,WCOL02+15 GET WADMSUB
   READ
   IF WADMSUB=SPACE(3) .OR. WADMSUB="000" .OR.READKEY()=12.OR.READKEY()=268
      LOOP
   ENDIF
   STORE WADMGRU+WADMSUB+"000" TO WCLAVE
   SELECT 2
   FIND &WCLAVE
   IF EOF()
      DO FILLOC
      APPEND BLANK
      UNLOCK
      DO RECLOC
      REPLACE GRUPO    WITH WADMGRU
      REPLACE SUBGRUPO WITH WADMSUB
      REPLACE RENGLON  WITH "000"
      FLUSH
      @ WLIN02+2,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
   ELSE
      do recloc
      @ WLIN02+2,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
      IF DESCRI = "*ELIMINA*"
         STORE "ESTA SEGURO QUE DESEA ELIMINAR ? (S/N)" TO TEX
         STORE "NS" TO WCH
         DO PREGUNTA
         IF WCH = "N"
            LOOP
         ELSE
            DO FILLOC
            DELETE
            UNLOCK
            LOOP
         ENDIF
      ENDIF
      FLUSH
   ENDIF
   @ WLIN02+3,WCOL02+15 GET WADMREN
   READ
   IF WADMREN=SPACE(3) .OR. WADMREN="000" .OR.READKEY()=12.OR.READKEY()=268
      LOOP
   ENDIF
   STORE WADMGRU+WADMSUB+WADMREN TO WCLAVE
   SELECT 2
   FIND &WCLAVE
   IF EOF()
      DO FILLOC
      APPEND BLANK
      UNLOCK
      DO RECLOC
      REPLACE GRUPO    WITH WADMGRU
      REPLACE SUBGRUPO WITH WADMSUB
      REPLACE RENGLON  WITH WADMREN
      FLUSH
      @ WLIN02+3,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
   ELSE
      do recloc
      @ WLIN02+3,WCOL02+20 GET DESCRI
      READ
      unlock
      IF READKEY()=12.OR.READKEY()=268
         LOOP
      ENDIF
      IF DESCRI = "*ELIMINA*"
         STORE "ESTA SEGURO QUE DESEA ELIMINAR ? (S/N)" TO TEX
         STORE "NS" TO WCH
         DO PREGUNTA
         IF WCH = "N"
            LOOP
         ELSE
            DO FILLOC
            DELETE
            UNLOCK
            LOOP
         ENDIF
      ENDIF
      FLUSH
   ENDIF
ENDDO
RETURN

   @ 15,55 PROMPT "REFERENCIAS DE ING/EGR"
   @ 16,55 PROMPT "FICHAS DE LOS BIENES  "
   MENU TO WSUB0301
   DO CASE
   CASE WSUB0301 = 1
        DO BIE0301A
   CASE WSUB0301 = 2
        DO BIE0301B
   CASE WSUB0301 = 3
        DO BIE0301C
   CASE WSUB0301 = 4
        DO BIE0301D
   CASE WSUB0301 = 5
        DO BIE0301E
   CASE WSUB0301 = 6
        DO BIE0301F
   CASE WSUB0301 = 7
        DO BIE0301G
   CASE WSUB0301 = 0
